{
	"$schema": "https://turbo.build/schema.json",
	"remoteCache": {
		"teamSlug": "ducktors",
		"apiUrl": "https://trrc.ducktors.dev"
	},
	"dangerouslyDisablePackageManagerCheck": true,
	"tasks": {
		"build": {
			"inputs": [
				"$TURBO_DEFAULT$",
				"../../tsup.config.ts",
				"../../*.json",
				"../../*.yaml",
				"../../*.ts",
				"../../*.mts",
				"../../*.mjs"
			],
			"dependsOn": ["^build"],
			"outputs": [
				"dist/**",
				".next/**",
				".source/**",
				".vercel/**",
				".output/**",
				".vinxi/**",
				"openapi.json"
			]
		},
		"generate": {
			"dependsOn": ["^generate"],
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"lint": {},
		"test": {}
	}
}
